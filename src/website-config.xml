<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="propertyConfigurer"
          class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
        <property name="location" value="classpath:config.properties"/>
    </bean>

    <bean id="WebsiteConfig" class="se.model.WebsiteConfig">
        <property name="pubSubjectStartTime" value="${pubSubjectStartTime}"/>
        <property name="pubSubjectEndTime" value="${pubSubjectEndTime}"/>
        <property name="selectSubjectStartTime" value="${selectSubjectStartTime}"/>
        <property name="selectSubjectEndTime" value="${selectSubjectEndTime}"/>
        <property name="uploadResultStartTime" value="${uploadResultStartTime}"/>
        <property name="uploadResultEndTime" value="${uploadResultEndTime}"/>
        <property name="pubGradeStartTime" value="${pubGradeStartTime}"/>
        <property name="pubGradeEndTime" value="${pubGradeEndTime}"/>
        <property name="checkInTime" value="${checkInTime}"/>
        <property name="checkOutTime" value="${checkOutTime}"/>
        <property name="checkInResetTime" value="${checkInResetTime}"/>
    </bean>
</beans>